<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Manual_Tax_and_Exempt_cant_check_same</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    Exempt__c = TRUE,
    Manual_Tax__c = TRUE
)</errorConditionFormula>
    <errorMessage>Please ensure only one checkbox is checked.</errorMessage>
</ValidationRule>
